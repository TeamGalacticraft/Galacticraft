{
  "multipart": [
    {"apply": {"model": "galacticraft:block/aluminum_wire"}},
    {"when":  {"north": "true"}, "apply":  {"model": "galacticraft:block/aluminum_wire_side"}},
    {"when":  {"east":  "true"}, "apply":  {"model": "galacticraft:block/aluminum_wire_side", "y": 90}},
    {"when":  {"south": "true"}, "apply":  {"model": "galacticraft:block/aluminum_wire_side", "y": 180}},
    {"when":  {"west":  "true"}, "apply":  {"model": "galacticraft:block/aluminum_wire_side", "y": 270}},
    {"when":  {"up":    "true"}, "apply":  {"model": "galacticraft:block/aluminum_wire_side", "x": 270, "y": 90}},
    {"when":  {"down":  "true"}, "apply":  {"model": "galacticraft:block/aluminum_wire_side", "x": 90, "y": 270}}
  ]
}