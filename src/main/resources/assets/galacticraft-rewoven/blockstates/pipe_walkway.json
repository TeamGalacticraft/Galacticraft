{
  "multipart": [
    {"apply": {"model": "galacticraft-rewoven:block/glass_fluid_pipe/none"}},

    {"when": {"facing": "up"},    "apply": {"model": "galacticraft-rewoven:block/pipe_walkway"}},
    {"when": {"facing": "down"},  "apply": {"model": "galacticraft-rewoven:block/pipe_walkway", "x": 180}},
    {"when": {"facing": "north"}, "apply": {"model": "galacticraft-rewoven:block/pipe_walkway", "x": 90}},
    {"when": {"facing": "south"}, "apply": {"model": "galacticraft-rewoven:block/pipe_walkway", "x": 270}},
    {"when": {"facing": "east"},  "apply": {"model": "galacticraft-rewoven:block/pipe_walkway", "x": 90, "y": 90}},
    {"when": {"facing": "west"},  "apply": {"model": "galacticraft-rewoven:block/pipe_walkway", "x": 90, "y": 270}},

    {"when": {"north": "true"}, "apply": {"model": "galacticraft-rewoven:block/glass_fluid_pipe/n"}},
    {"when": {"east": "true"},  "apply": {"model": "galacticraft-rewoven:block/glass_fluid_pipe/n", "y": 90}},
    {"when": {"south": "true"}, "apply": {"model": "galacticraft-rewoven:block/glass_fluid_pipe/n", "y": 180}},
    {"when": {"west": "true"},  "apply": {"model": "galacticraft-rewoven:block/glass_fluid_pipe/n", "y": 270}},
    {"when": {"up": "true"},    "apply": {"model": "galacticraft-rewoven:block/glass_fluid_pipe/n", "x": 270, "y": 90}},
    {"when": {"down": "true"},  "apply": {"model": "galacticraft-rewoven:block/glass_fluid_pipe/n", "x": 90, "y": 270}}
  ]
}
